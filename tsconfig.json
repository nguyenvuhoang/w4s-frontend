{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@configs/*": [
        "./src/configs/*"
      ],
      "@core/*": [
        "./src/@core/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@views/*": [
        "./src/views/*"
      ],
      "@components/*": [
        "./src/@menu/components/*",
        "./src/shared/components/*"
      ],
      "@components": [
        "./src/shared/components"
      ],
      "@utils/*": [
        "./src/@menu/utils/*",
        "./src/shared/utils/*"
      ],
      "@utils": [
        "./src/shared/utils"
      ],
      "@hooks/*": [
        "./src/@menu/hooks/*",
        "./src/shared/hooks/*"
      ],
      "@hooks": [
        "./src/shared/hooks"
      ],
      "@contexts/*": [
        "./src/@menu/contexts/*",
        "./src/shared/contexts/*"
      ],
      "@contexts": [
        "./src/shared/contexts"
      ],
      "@layouts/*": [
        "./src/@layouts/*"
      ],
      "@menu/*": [
        "./src/@menu/*"
      ],
      "@lib/*": [
        "./src/libs/*"
      ],
      "@features/*": [
        "./src/features/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@shared": [
        "./src/shared"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next"
  ]
}